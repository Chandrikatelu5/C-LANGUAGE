#include<stdio.h>
void main()
{
    char a[10][10];
    int b;
    printf("Enter no.of names:");
    scanf("%d",&b);
    printf("Enter names:");
    for(int i=0;i<b;i++)
    {
        scanf("%s",a[i]);
    }
    printf("Names:\n");
    for(int i=0;i<b;i++)
    {
        printf("No. %d - %s",i+1,a[i]);
        printf("\n");
    }
}
