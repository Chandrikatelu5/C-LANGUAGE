#include<stdio.h>
void main() 
{
    int n1 = 0;
    int n2 = 1;
    int r, temp, c;
    printf("Enter range: ");
    scanf("%d", &r);
    printf("%d %d", n1, n2);
    for (int i = 3; i <= r; i++) 
    {
        c = n1 + n2;
        printf(" %d", c);
        temp = n1;
        n1 = n2;
        n2 = c;
    }
}
