#include <stdio.h>
void main()
{
    int a,b,c;
    printf("Enter three no.:");
    scanf("%d%d%d",&a,&b,&c);
    if(a>b && a>c)
    {
        if(b>c)
        printf("%d is second greatest",b);
        else
        printf("%d is second greatest",c);
    }
    else if(b>a && b>c)
    {
        if(a>c)
        printf("%d is second greatest",a);
        else
        printf("%d is second greatest",c);
    }
    else
    {
        if(b>a)
        printf("%d is second greatest",b);
        else
        printf("%d is second greatest",a);
    }
}
