#include<stdio.h>
void main()
{
    int a[20],l;
    printf("Enter no.of elements:");
    scanf("%d",&l);
    printf("Enter %d elements:",l);
    for(int i=0;i<l;i++)
    scanf("%d",&a[i]);
    int max=a[0];
    for(int i=1;i<l;i++)
    {
        if(max<a[i])
        max=a[i];
    }
    printf("Max element:%d",max);
}
