#include <stdio.h>
void main()
{
    int i,j,c,low,high; 
    printf("enter lower range: ");
    scanf("%d",&low); 
    printf("enter upper range : "); 
    scanf("%d",&high);
    for(i=low; i <= high; i++) 
    {
        c= 0;
        for(j= 2; j<= i/2;j++) 
        {
            if(i % j == 0) 
            {
                c++; 
                break; 
            }
        }

        if(c == 0 && i != 1) 
            printf("%d ",i);
    }
    printf("\n");
}
