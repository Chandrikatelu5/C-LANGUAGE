#include<stdio.h>
void prime(int n);
void main()
{
    int n;
    printf("Enter range:");
    scanf("%d",&n);
    prime(n);
}
void prime(int n)
{
    int count=0;
    if(n!=1)
    {
    for(int i=2;i<n;i++)
    {
        if(n%i==0)
            count=1;
    }
    if(count==0)
        printf("  %d",n);
    prime(n-1);
    }
    return;
}
