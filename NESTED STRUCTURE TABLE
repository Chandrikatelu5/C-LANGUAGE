#include<stdio.h>  
struct marks   
{  
    int mark1;;  
    int mark2;
};  
struct details  
{  
    char name[20];
    int rollno;
    struct marks m;  
};  
void main ()  
{  
    struct details p[20];  
    for(int i=0;i<2;i++)
    {
        printf("Enter %d name:",i+1);
        scanf("%s",p[i].name);
        printf("Enter %d roll:",i+1);
        scanf("%d",&p[i].rollno);
        printf("Enter %d mark1:",i+1);
        scanf("%d",&p[i].m.mark1);
        printf("Enter %d mark2:",i+1);
        scanf("%d",&p[i].m.mark2);
    }
    printf("\nNAME\tROLLNO\tMARK1\tMARK2");
    for(int i=0;i<2;i++)
    {
        printf("\n%s",p[i].name);
        printf("\t\t%d",p[i].rollno);
        printf("\t\t%d",p[i].m.mark1);
        printf("\t\t%d",p[i].m.mark2);
    }
}  
