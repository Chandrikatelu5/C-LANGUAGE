#include<stdio.h>
int add(int a,int b);
void main()
{
    int a,b;
    printf("Enter two no:");
    scanf("%d%d",&a,&b);
    for(int i=0;i<=add(a,b);i++)
    {
        for(int j=0;j<i;j++)
        {
            printf(" *");
        }
        printf("\n");
    }
}
int add(int a,int b)
{
    return a+b;
}
